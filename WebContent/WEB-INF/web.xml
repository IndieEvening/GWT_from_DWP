<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>GWT_from_DWP</display-name>

	<!-- Servlets -->
	<servlet>
		<servlet-name>customerService</servlet-name>
		<servlet-class>com.myGWTapp.server.AddCustomerServiceImp</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>customerService</servlet-name>
		<url-pattern>/myGWTapp/addcustomer</url-pattern>
	</servlet-mapping>
  
	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>Wyklad_14_06.html</welcome-file>
	</welcome-file-list>
</web-app>